{
    "user": "Y-Less",
    "repo": "micro",
    "contributors": ["Y_Less"],
    "resources": [
        {
            "name": "^micro-(.+)-linux.tar.gz$",
            "platform": "linux",
            "archive": true,
            "includes": ["pawno/include"],
            "plugins": ["plugins/micro.so"]
        },
        {
            "name": "^micro-(.*)-win32.zip$",
            "platform": "windows",
            "archive": true,
            "includes": ["pawno/include"],
            "plugins": ["plugins/micro.dll"]
        }
    ],
    "runtime": {
        "plugins": ["Y-Less/micro"]
    }
}
